<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ym.mybatis.mapper.PersonMapper">

    <select id="getPersonById" resultType="ym.mybatis.domain.Person">
        select * from person where id = #{id}
    </select>

    <insert id="insertPerson" parameterType="ym.mybatis.domain.Person">
        insert into person (name,sex)
            values (#{name},#{sex})
    </insert>
</mapper>